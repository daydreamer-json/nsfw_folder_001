	// ------------------------------------------------------------------
	// フィルタ用コマンド定義
	// ------------------------------------------------------------------

	"filterList" => [
		%[
			// 名前指定
			"name", "allimage",
			// フィルタ指定: 文字列:他の定義を参照 関数:帰り値が真なら参照 正規表現:名前でマッチング。配列で複数指定可能
			"filter", function(obj, elm) { return obj instanceof "KAGEnvImage" && (elm.env === void || elm.env == obj.env.name); },
			// コマンド実行前に排除するタグ・判定などで使ったものを誤動作防止で処理。配列で複数指定可能。
			"remove", "env",
		],
		%[
			"name", "allchar",
			// 継承すると親の条件を満たした後追加でしぼりこむ
			"parent", "allimage",
			"filter", function(obj,elm) { return obj.className == "character"; }
		],
		%[
			"name", "allbase",
			"parent", "allimage",
			"filter", function(obj,elm) { return obj.className == "stage" || obj.className == "event"; }
		],
		%[
			"name", "allse",
			"filter", function(obj,elm) { return obj.className == "se" || obj.className == "lse"; },
		]
	],

