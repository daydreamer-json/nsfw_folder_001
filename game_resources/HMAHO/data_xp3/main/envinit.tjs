	// レイヤ系スケーリング指定
	// スクリプトでの座標値と実画面での座標値のスケーリング処理を行う
	//"scalex", 1.6,    X方向のスケール(default:1.0)
	//"scaley", 1.6,    Y方向のスケール(default:1.0)
	//"width" , 800,    仮想画面サイズ横幅(default:scWidth/scalex)
	//"height", 600,    仮想画面サイズ縦幅(default:scHeight/scaley)

	// 舞台情報定義
	// 基準になる stage ファイルを指定する
	"stageInfo" => "base.stage",
	
	// stage/stime/アクションを直接コマンドとして使った場合の
	// デフォルトターゲットを変更
	// ※未指定時は環境に対して処理される
	"defaultTarget" => "bg",

	// posのカンマ指定の3つ目のパラメータをzorderにする
	"posInfoZorder" => true,
	
	// ------------------------------------------------------------------
	// レイヤ配置情報定義
	// disp
	// xpos
	// ypos
	// zpos
	// reset
	// front
	// back

	// レイヤ配置情報定義
	// disp
	// xpos
	// ypos
	// zpos
	// reset
	// front
	// back

	// ------------------------------------------------------------------

	// autoface.tjs 設定
	"autoFaceShow",  true,  // 顔窓自動表示	 false :無効							[true]:有効(話者にあわせて表示制御する)
	"syncFaceShow",  true,  // face同期表示	 false :顔位置オブジェクトのみ対象		[true]:立ち絵に同期した顔窓表示を追加する
//	"eventFaceShow", false, // イベント時	[false]:イベント絵時は自動表示しない	 true :イベント絵時も自動表示する
//	"forceFaceShow", false, // 強制表示		[false]:顔窓を立ち絵の表示状態に合わせる true :立ち絵が消えていても表示する
	"facePosMode", 0, // 座標を実行時に補正

	// レベル番号とzorderの対応を記載する
	"levels" => [ 66,100,200,225 ],
	
	"positions" => %[
		// ※type定義は不要になりました。disp/xpos/ypos/level いずれかの定義が記述されていればそれが使用されます。

		// 表示定義（顔表示は，フェイスウィンドウ機能がない場合には意味を持ちません）
		"出" => %[ "disp" => KAGEnvImage.SHOW      ], // 表示
		"消" => %[ "disp" => KAGEnvImage.CLEAR     ], // 一時非表示（状態変更による自動表示は有効）※基本はこちらを使います
		"無" => %[ "disp" => KAGEnvImage.INVISIBLE ], // 完全非表示（状態変更による自動表示も無効）

		"顔" => %[ "disp" => KAGEnvImage.SHOW, "className", "msgwin" ], // 顔配置用クラス指定
		"立" => %[ "disp" => KAGEnvImage.SHOW, "className", "bu" ], // 立ち絵のみ表示用クラス指定

		// トランジション指定つき
		"出左"   => %[ "disp" => KAGEnvImage.SHOW, trans:"trans左" ], // 表示
		"出左速" => %[ "disp" => KAGEnvImage.SHOW, trans:"trans左速" ], // 表示
		"出左遅" => %[ "disp" => KAGEnvImage.SHOW, trans:"trans左遅" ], // 表示
		"消左"   => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans左" ], // 消去
		"消左速" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans左速" ], // 消去
		"消左遅" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans左遅" ], // 消去

		"出右"   => %[ "disp" => KAGEnvImage.SHOW, trans:"trans右" ], // 表示
		"出右速" => %[ "disp" => KAGEnvImage.SHOW, trans:"trans右速" ], // 表示
		"出右遅" => %[ "disp" => KAGEnvImage.SHOW, trans:"trans右遅" ], // 表示
		"消右"   => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans右" ], // 消去
		"消右速" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans右速" ], // 消去
		"消右遅" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans右遅" ], // 消去

		"立ち上がり"     => %[ "disp" => KAGEnvImage.SHOW, trans:"trans立ち上がり" ], // 表示
		"立ち上がり速"   => %[ "disp" => KAGEnvImage.SHOW, trans:"trans立ち上がり速" ], // 表示
		"立ち上がり遅"   => %[ "disp" => KAGEnvImage.SHOW, trans:"trans立ち上がり遅" ], // 表示
		"立ち上がり消"   => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans立ち上がり" ], // 消去
		"立ち上がり消速" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans立ち上がり速" ], // 消去
		"立ち上がり消遅" => %[ "disp" => KAGEnvImage.CLEAR, trans:"trans立ち上がり遅" ], // 消去

		"しゃがみ"     => %[ "disp" => KAGEnvImage.SHOW, trans:"transしゃがみ" ], // 表示
		"しゃがみ速"   => %[ "disp" => KAGEnvImage.SHOW, trans:"transしゃがみ速" ], // 表示
		"しゃがみ遅"   => %[ "disp" => KAGEnvImage.SHOW, trans:"transしゃがみ遅" ], // 表示
		"しゃがみ消"   => %[ "disp" => KAGEnvImage.CLEAR, trans:"transしゃがみ" ], // 消去
		"しゃがみ消速" => %[ "disp" => KAGEnvImage.CLEAR, trans:"transしゃがみ速" ], // 消去
		"しゃがみ消遅" => %[ "disp" => KAGEnvImage.CLEAR, trans:"transしゃがみ遅" ], // 消去

		// 位置定義
		"左外" => %[ "xpos" => int -600 ],
		"左奥" => %[ "xpos" => int -440 ],
		"左"   => %[ "xpos" => int -250 ],
		"左中" => %[ "xpos" => int -100 ],
		"中"   => %[ "xpos" => int 0    ],
		"右中" => %[ "xpos" => int  100 ],
		"右"   => %[ "xpos" => int 250 ],
		"右奥" => %[ "xpos" => int  440 ],
		"右外" => %[ "xpos" => int  600 ],
		
		// yposは直で個別に指定する場合が多いようです
		//"縦デフォルト" => %[ "ypos" => int    0 ],
		//"縦しゃがみ"   => %[ "ypos" => int   50 ],
		//"縦背伸び"     => %[ "ypos" => int  -20 ],

		// レベル指定 + リセット処理
		"奥"   => %[ zorder: int 66,  ypos:0, reset:true, front:true, className:"" ],
		"前"   => %[ zorder: int 100, ypos:0, reset:true, front:true, className:"" ],
		"手前" => %[ zorder: int 200, ypos:0, reset:true, front:true, className:"" ],
		"目前" => %[ zorder: int 225, ypos:0, reset:true, front:true, className:"" ],

		],

	// 自動レイアウト指定
	"layouts" => %[
		"入場" => %[ "type", "normal", "pos", "right", "trans", "showslide" ],
		"割込" => %[ "type", "normal", "pos", "center", "trans", "showslide" ],
		"退場" => %[ "type", "normal", "pos", "hide", "trans", "showslide" ],
		// 入退場定義・サイズ補正あり
		"入場2" => %[ "type", "autolevel", "pos", "right", "reload", true, "trans", "showslide" ],
		"退場2" => %[ "type", "autolevel", "pos", "hide", "reload", true, "trans", "showslide" ],
	],

	// レイアウト用座標定義・レベルあり
	"layouts_autolevel" => [
		%[ "xpos", [0], "level", 2, "leveltrans", "showslide", "time", 300 ],
		%[ "xpos", [-250,250], "level", 1, "leveltrans", "showslide", "time", 300 ],
		%[ "xpos", [-400,0,400], "level", 1, "leveltrans", "showslide", "time", 300 ],
		%[ "xpos", [-500,-200,200,500], "level", 1, "leveltrans", "showslide", "time", 300 ],
	],
	// レイアウト用座標定義・レベル無し
	"layouts_normal" => [
		%[ "xpos", [0], "time", 300],
		%[ "xpos", [-250,250], "time", 300 ],
		%[ "xpos", [-400,0,400], "time", 300  ],
		%[ "xpos", [-500,-200,200,500], "time", 300  ],
	],
	
	// ------------------------------------------------------------------
	// 加工定義
	// ------------------------------------------------------------------

	"modifies" => %[
		"セピア化" => %[grayscale:true, rgamma:1.3, ggamma:1.1],
		"色違い" => %[hue:-30],
		],

	// ------------------------------------------------------------------
	// アクション定義
	// ------------------------------------------------------------------
	
	"actions" => %[

		// --------------------------------------------------------
		// 旧スタイル定義
		// --------------------------------------------------------

		"どっきり" => %[
			"module" => "LayerHeartBeatZoomModule",
			"time" => 200,
			"zoom" => 110,
			],
		"どっきり小" => %[
			"module" => "LayerHeartBeatZoomModule",
			"time" => 100,
			"zoom" => 103,
			],
		"どっきり大" => %[
			"module" => "LayerHeartBeatZoomModule",
			"time" => 150,
			"zoom" => 150,
			],
		"どっきり速" => %[
			"module" => "LayerHeartBeatZoomModule",
			"time" => 100,
			"zoom" => 108,
			],
		"びょん" => %[
			"module" => "LayerJumpActionModule",
			"vibration" => -30,
			"cycle" => 1000,
			],
		"びょん小" => %[
			"module" => "LayerJumpActionModule",
			"vibration" => 6,
			"cycle" => 500,
			],
		"びょん速" => %[
			"module" => "LayerJumpActionModule",
			"vibration" => 3,
			"cycle" => 300,
			"nowait" => true,
			],
		"びょん速−" => %[
			"module" => "LayerJumpActionModule",
			"vibration" => -3,
			"cycle" => 300,
			"nowait" => true,
			],
		"びょん待ち無" => %[
			"module" => "LayerJumpActionModule",
			"vibration" => -30,
			"cycle" => 1000,
			"nowait" => true,
			],
		
		// --------------------------------------------------------
		// 新スタイル定義
		// --------------------------------------------------------

		"移動テスト" => %[ // 現在位置から+300の位置に500ms かけて移動
						   "time" => 500,
						   "xpos" => %[ handler:"MoveAction", value:"@+300" ],
						   ],

		"パス移動テスト" => %[ // 複数移動の一括指定
						   "time" => 1000,
						   "xpos" => %[ handler:"PathAction", value:"@+300,@-300,@+100" ],
						   ],

		"パス移動テスト2" => %[ // KAGのmove互換表記
								"time" => 3000,
								"path" => "(-400,400,128)(-400,-400,100)(0,0,256)",
								"spline" => true,
						   ],
		
		"回転テスト" => %[ // 1000msの間回転
						   "time" => 1000,
						   "rotate" => %[ handler:"TriangleAction", vibration:360,  ontime:1000, offtime:0 ]
						   ],

		"スローズーム" => %[ // 20000msの間ちょっとずつズーム
						   "time" => 20000,
						   "zoom" => %[ handler:"MoveAction", value:110 ]
						   ],

		// タイル系画像でのみ意味のあるアクション
		"タイルスライド" => %[
			"$tilex" => %[ handler:"LoopMoveAction", max:800, loop:2000 ],
			"$tiley" => %[ handler:"LoopMoveAction", max:600, loop:2000 ]
			],

		"横スライド" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:-5120, loop:1280 ],
			"nowait" => true,
			],

		"横スライド速" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:-7680, loop:1280 ],
			"nowait" => true,
			],

		"横スライド遅" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:-2560, loop:1280 ],
			"nowait" => true,
			],

		"横スライド逆" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:5120, loop:1280 ],
			"nowait" => true,
			],

		"横スライド逆速" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:7680, loop:1280 ],
			"nowait" => true,
			],

		"横スライド逆遅" => %[
			"$tilex"  => %[ handler:"LoopMoveAction", max:2560, loop:1280 ],
			"nowait" => true,
			],

		"縦スライド" => %[
			"$tiley"  => %[ handler:"LoopMoveAction", max:2400, loop:800 ],
			"nowait" => true,
			],

		"縦スライド速" => %[
			"$tiley"  => %[ handler:"LoopMoveAction", max:4800, loop:800 ],
			"nowait" => true,
			],

		"ゆさゆさ" => %[
			"xpos"	=> %[ handler:"SinAction", vibration: -12, cycle: "%cycle|800", ],
			"ypos"	=> %[ handler:"SinAction", vibration: 4, cycle: "%cycle|800", ],
			 ],
		
		"カメラガクガク" => %[
			"camerax"	=> %[ handler:"RandomAction", vibration:10, waittime:20],
			"cameray"	=> %[ handler:"RandomAction", vibration:10, waittime:20],
			],

		"シフトガクガク" => %[
			"shiftx"	=> %[ handler:"RandomAction", vibration:10, waittime:20],
			"shifty"	=> %[ handler:"RandomAction", vibration:10, waittime:20],
			],

		"右往左往" => [
			// c200.ks: [bg path=(-800,0,256)(800,0,256)(0,0,256) time=5000 nowait] 置き換え
			%[ "left" => %[ handler:"MoveAction", value:"@-800", time:5000\3, accel:"acdec"] ],
			%[ "left" => %[ handler:"MoveAction", value:"@+800", time:5000\3, accel:"acdec"] ],
			%[ "left" => %[ handler:"MoveAction", value:"@0",    time:5000\3, accel:"acdec"] ],
			],

		//↓旧スタイルのアクションを新スタイルで書き直したもの----------------------------------------------------

		"おじぎ" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-25", time:500, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:500, accel:1,] ],
			],

		"おじぎ小" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-10", time:300, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:300, accel:1] ],
			],

		"おじぎ大" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-50", time:1000, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:1000, accel:1] ],
			],

		"びょん" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@+6", time:100, accel:-2] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@-3", time:150, accel:1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:50, accel:-1], nowait:true ],
			],

		"びょん大" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@+60", time:150, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@-3", time:200, accel:1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:50, accel:-1], nowait:true ],
			],

		"ジャンプ" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@+10", time:125, accel:-2] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:125, accel:1] ],
			],

		"ジャンプ大" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-5", time:100, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@+60", time:300, accel:-1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@-5", time:200, accel:1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:100, accel:-1] ],
			],

		"浮遊" => %[
			"ypos"	=> %[ handler:"SinAction", vibration: "%vibration|15", cycle:"%cycle|4000", nowait:true ],
			 ],
		"浮遊小" => %[
			"ypos"	=> %[ handler:"SinAction", vibration: "%vibration|3", cycle:"%cycle|2500", nowait:true ],
			 ],

		"浮遊速" => %[
			"ypos"	=> %[ handler:"SinAction", vibration: "%vibration|10", cycle:"%cycle|500", nowait:true ],
			 ],

		"腰振り" => %[
			"ypos"	=> %[ handler:"SinAction", vibration: "%vibration|10", cycle:"%cycle|1500", nowait:true ],
			 ],

		"ランニング" => %[
			"ypos"	=> %[ handler:"SinAction", vibration: "%vibration|3", cycle:"%cycle|250", nowait:true ],
			 ],

		"うろうろ" => %[
			"xpos"	=> %[ handler:"SinAction", vibration: "%vibration|50", cycle:"%cycle|5000", nowait:true  ],
			 ],

		"うろうろ大" => %[
			"xpos"	=> %[ handler:"SinAction", vibration: "%vibration|100", cycle:"%cycle|5000", nowait:true  ],
			 ],

		"うろうろ小" => %[
			"xpos"	=> %[ handler:"SinAction", vibration: "%vibration|10", cycle:"%cycle|500", nowait:true  ],
			 ],

		"うろうろ遅" => %[
			"xpos"	=> %[ handler:"SinAction", vibration: "%vibration|5", cycle:"%cycle|2500", nowait:true  ],
			 ],

		"ガクガク" => %[
			"xpos"	=> %[ handler:"RandomAction", value:"2", waittime:"30", nowait:true ],
			"ypos"	=> %[ handler:"RandomAction", value:"2", waittime:"30", nowait:true ],
			 ],

		"ガクガク大" => %[
			"xpos"	=> %[ handler:"RandomAction", value:"4", waittime:"50", nowait:true ],
			"ypos"	=> %[ handler:"RandomAction", value:"4", waittime:"50", nowait:true ],
			 ],
		//↑----------------------------------------------------------------------------------------------------

		"背伸び" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@+5", time:1000, accel:"acdec"] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@+3", time:300, accel:1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:500, accel:-1] ],
			],
		
		"がっくり" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-10", time:500, accel:1] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@-5", time:300, accel:-1] ],
			],

		"ふるふる" => [
			%[ "xpos" => %[ handler:"MoveAction", value:"@-5*mag", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+5*mag", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@-5*mag", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+5*mag", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@", time:200, accel:"acdec"] ],
			],

		"ふるふる速" => [
			%[ "xpos" => %[ handler:"MoveAction", value:"@-5", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+5", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@-5", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+5", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@", time:100, accel:"acdec"] ],
			],

		"ふるふる小" => [
			%[ "xpos" => %[ handler:"MoveAction", value:"@-2", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+2", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@-2", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+2", time:200, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@", time:200, accel:"acdec"] ],
			],

		"ふるふる小速" => [
			%[ "xpos" => %[ handler:"MoveAction", value:"@-2", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+2", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@-2", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@+2", time:100, accel:"acdec"] ],
			%[ "xpos" => %[ handler:"MoveAction", value:"@", time:100, accel:"acdec"] ],
			],

		"こくこく" => [
			%[ "ypos" => %[ handler:"MoveAction", value:"@-10", time:200, accel:"acdec"] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:200, accel:"acdec"] ],
			%[ wait:5 ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@-10", time:200, accel:"acdec"] ],
			%[ "ypos" => %[ handler:"MoveAction", value:"@", time:200, accel:"acdec"] ],
			],

		"はぁはぁ" => [
			%[ "top" => %[ handler:"MoveAction", value:"@-10", time:500, accel:"acdec"] ],
			%[ "top" => %[ handler:"MoveAction", value:"@", time:500, accel:"acdec"] ],
			%[ wait:5 ],
			%[ "top" => %[ handler:"MoveAction", value:"@-10", time:500, accel:"acdec"] ],
			%[ "top" => %[ handler:"MoveAction", value:"@", time:500, accel:"acdec"] ],
